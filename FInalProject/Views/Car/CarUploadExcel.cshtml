@model FInalProject.Models.Car

@{
     ViewData["Title"] = "getExcelUrl";
}

<h1>Изменение данных</h1>

<h4>Автомобиль</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-action="getExcelUrl">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                                 <span>Select File:</span>
                                    <input type="file" name="postedFiles" multiple/>
                                    <a asp-area="" asp-controller="Car" asp-action="GetExcelUrl" >Выгрузить таблицу</a>
                                
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="CarTablePage">Вернуться назад</a>
</div>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}